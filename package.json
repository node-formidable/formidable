{
  "name": "formidable-monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "A monorepo for Node Formidable packages",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "git clean -fdX .",
    "publish-release": "node --env-file .env ./scripts/publish-release.js",
    "publish-release-ci": "node ./scripts/publish-release.js",
    "tag-release": "node ./scripts/tag-release.js",
    "test": "workspaces-filter '*' test"
  },
  "dependencies": {
    "@types/bun": "1.2.10",
    "@types/node": "22.14.1",
    "prettier": "3.5.3",
    "typescript": "^5.8.3",
    "workspaces-filter": "^0.7.3"
  },
  "devDependencies": {
    "eslint": "^9.26.0",
    "eslint-config-xaxa": "^28.0.3"
  }
}
